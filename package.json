    "brace": "^0.11.1",
    "core-js": "^3.6.4",
    "lodash-es": "^4.17.15",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
